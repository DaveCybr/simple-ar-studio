{
  "public": "dist",
  "rewrites": [
    {
      "source": "/ar-viewer.html",
      "destination": "/ar-viewer.html"
    },
    {
      "source": "/ar.html",
      "destination": "/ar.html"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "**/*.@(jpg|jpeg|gif|png|svg|ico|webp)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
